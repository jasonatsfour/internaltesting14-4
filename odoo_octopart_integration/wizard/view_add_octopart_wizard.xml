<odoo>
    <data>

        <record id="add_octopart_wizard_view" model="ir.ui.view">
            <field name="name">add.octopart.wizard.form</field>
            <field name="model">add.octopart.wizard</field>
            <field name="arch" type="xml">
                <form string="Update Octopart Vendor Id">
                    <p class="oe_grey">
                        This will set selected vendor octopart id to current
                        vendor.
                    </p>
                    <group>
                        <field name="octopart_id"
                               options="{'no_create_edit': True, 'no_quick_create':True, 'no_open': True}"/>
                    </group>
                    <footer>
                        <button string="Set Vendor Id"
                                name="set_vendor_octopart_id"
                                type="object" default_focus="1" class="btn-primary"/>
                        <button string="Cancel" class="btn-default" special="cancel"/>
                    </footer>
                </form>
            </field>
        </record>

        <record id="add_octopart_id_search" model="ir.ui.view">
            <field name="model">add.octopart.id</field>
            <field name="arch" type="xml">
                <search string="Octopart">
                    <field name="name"
                           filter_domain="['|', ('name','ilike',self), ('uid','ilike',self)]" string="Vendor"/>
                    <field name="sku" string="SKU"
                            filter_domain="[('sku','ilike',self)]"/>
                    <field name="min_qty" string="MOQ"
                            filter_domain="[('min_qty','ilike',self)]"/>
                </search>
            </field>
        </record>

        <record id="add_octopart_id_tree_view" model="ir.ui.view">
            <field name="name">add.octopart.wizard.tree</field>
            <field name="model">add.octopart.id</field>
            <field name="arch" type="xml">
                <tree string="Update Octopart Vendor Id" create="false" edit="false">
                    <field name="name"/>
                    <field name="uid" invisible="1"/>
                    <field name="sku"/>
                    <field name="stock"/>
                    <field name="min_qty"/>
                    <field name="pkg"/>
                    <field name="currency_id"/>
                    <field name="qty_1"/>
                    <field name="qty_10"/>
                    <field name="qty_100"/>
                    <field name="qty_1000"/>
                    <field name="qty_10000"/>
                    <field name="last_updated"/>
                    <field name="product_tmpl_id" invisible="1"/>

                </tree>
            </field>
        </record>

         <record id="action_octopart_vendor_id_Assignment"
                model="ir.actions.act_window">
            <field name="name">Octopart Vendor Id Assignment Wizard</field>
            <field name="res_model">add.octopart.wizard</field>
            <field name="view_mode">form</field>
        </record>

    </data>
</odoo>
