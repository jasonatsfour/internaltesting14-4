<odoo>
    <data>

        <record id="fetch_seller_detail_tree_view" model="ir.ui.view">
            <field name="name">fetch.seller.detail.tree</field>
            <field name="model">fetch.seller.detail</field>
            <field name="arch" type="xml">
                <form string="Fetch Seller Detail">
                    <p>
                        Fetch vendor details form Octopart for selected
                        products
                    </p>
                    <footer>
                        <button string="Fetch Vendors"
                                name="fetch_seller_details"
                                type="object" default_focus="1"
                                class="btn-primary"/>
                        <button string="Cancel" class="btn-default"
                                special="cancel"/>
                    </footer>
                </form>
            </field>
        </record>

        <record id="action_fetch_seller_detail" model="ir.actions.act_window">
            <field name="name">Fetch Vendors Details</field>
            <field name="res_model">fetch.seller.detail</field>
            <field name="view_mode">form</field>
            <field name="target">new</field>
            <field name="binding_model_id" ref="model_fetch_seller_detail"/>
        </record>

        <record id="fetch_sellers_view_tree" model="ir.ui.view">
            <field name="name">fetch.sellers.tree</field>
            <field name="model">fetch.sellers</field>
            <field name="arch" type="xml">
                <form string="Fetch Seller Detail">
                    <p>
                        Fetch vendor details form Octopart for selected
                        products
                    </p>
                    <footer>
                        <button string="Fetch Vendors"
                                name="fetch_seller_details"
                                type="object" default_focus="1"
                                class="btn-primary"/>
                        <button string="Cancel" class="btn-default"
                                special="cancel"/>
                    </footer>
                </form>
            </field>
        </record>


        <record id="action_fetch_sellers_view" model="ir.actions.act_window">
            <field name="name">Fetch Vendors Details</field>
            <field name="res_model">fetch.sellers</field>
            <field name="view_mode">form</field>
            <field name="target">new</field>
            <field name="binding_model_id"
                   ref="product.model_product_template"/>
        </record>

     </data>
</odoo>
